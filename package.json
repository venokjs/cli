{
  "name": "@venok/cli",
  "version": "1.0.0",
  "description": "Venok cli",
  "author": "0x303133",
  "bin": "dist/bin/venok.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venokjs/cli.git"
  },
  "bugs": {
    "url": "https://github.com/venokjs/cli/issues"
  },
  "homepage": "https://github.com/venokjs/cli#readme",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "rimraf ./dist",
    "start": "node ./dist/bin/venok.js",
    "test": "jest --config test/jest-config.js",
    "test:dev": "npm run clean && jest --config test/jest-config.js --watchAll",
    "lint": "eslint \"{actions,commands,src,test}/**/*.ts\" --fix",
    "format": "prettier --write \"**/*.ts\"",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "venok",
    "venokjs",
    "cli"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.73",
    "@swc/jest": "^0.2.27",
    "@types/jest": "^29.5.3",
    "chalk": "4.1.2",
    "commander": "^11.0.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/node": "^20.4.6",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.0",
    "release-it": "^16.1.3",
    "ts-jest": "^29.1.1",
    "tsc": "^2.0.4"
  }
}
